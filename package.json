{
  "private": true,
  "name": "lifetime-app",
  "version": "0.0.1",
  "scripts": {
    "ruby:bundle": "bundle install",
    "cocoapods:install": "cd ios && bundle exec pod install --repo-update",
    "svg:cleanup": "trash src/SVGs/components",
    "svg:convert": "react-from-svg src/SVGs src/SVGs/components --with-native-for-reason --remove-fill",
    "svg:format": "find src/SVGs/components -name \"*.re\" | grep -v \"node_modules\" | xargs bsrefmt --in-place",
    "svg": "yarn svg:cleanup && yarn svg:convert && yarn svg:format",
    "bs:clean": "bsb -make-world -clean-world",
    "bs:build": "bsb -make-world",
    "md:to-json": "markdown-to-json src/md/*.md",
    "prepare": "patch-package && yarn ruby:bundle && yarn cocoapods:install && yarn svg && yarn md:to-json && yarn bs:clean && yarn bs:build",
    "start": "react-native start",
    "start:android": "react-native run-android",
    "start:ios": "react-native run-ios --simulator=\"iPhone 11\"",
    "start:web": "react-scripts start",
    "build:web": "react-scripts build",
    "test:native": "jest",
    "test:web": "react-scripts test",
    "lint": "eslint --ignore-path .gitignore .",
    "test:bundle:ios": "react-native bundle --platform ios --dev false --minify false --entry-file index.js --bundle-output index.bundle.js && rm index.bundle.js",
    "test:bundle:android": "react-native bundle --platform android --dev false --minify false --entry-file index.js --bundle-output index.bundle.js && rm index.bundle.js",
    "test:bundle": "yarn test:bundle:ios && yarn test:bundle:android",
    "test": "yarn lint && yarn bs:clean && yarn test:bundle"
  },
  "dependencies": {
    "@ctrl/tinycolor": "^2.6.0",
    "@glennsl/bs-json": "^5.0.1",
    "@react-native-community/async-storage": "^1.7.1",
    "@react-native-community/blur": "^3.4.1",
    "@react-native-community/masked-view": "^0.1.5",
    "@react-native-community/slider": "^2.0.0",
    "@react-navigation/bottom-tabs": "^5.7.0",
    "@react-navigation/native": "^5.7.0",
    "@react-navigation/stack": "^5.7.0",
    "@reason-react-native/async-storage": "^1.6.0",
    "@reason-react-native/permissions": "^2.0.0",
    "@reason-react-native/safe-area-context": "^3.0.0",
    "@reason-react-native/slider": "^3.0.0",
    "@sentry/react-native": "^1.6.2",
    "bs-tinycolor": "^2.0.1",
    "date-fns": "^2.8.1",
    "react": "16.11.0",
    "react-dom": "^16.11.0",
    "react-multiversal": "https://github.com/MoOx/react-multiversal#bf0f30f",
    "react-native": "^0.62.0",
    "react-native-bootsplash": "^2.0.0",
    "react-native-calendar-events": "https://github.com/wmcmahan/react-native-calendar-events.git#504ccff",
    "react-native-dark-mode": "^0.2.1",
    "react-native-gesture-handler": "^1.6.1",
    "react-native-localize": "^1.4.0",
    "react-native-permissions": "^2.0.8",
    "react-native-reanimated": "^1.9.0",
    "react-native-redash": "^9.6.0",
    "react-native-safe-area-context": "^3.0.0",
    "react-native-screens": "^2.9.0",
    "react-native-svg": "^12.0.0",
    "react-native-web": "^0.12.0",
    "reason-date-fns": "^0.10.4",
    "reason-future": "^2.5.0",
    "reason-react": "^0.8.0",
    "reason-react-native": "0.62.0",
    "reason-react-navigation": "^5.0.0-beta.0"
  },
  "devDependencies": {
    "@babel/core": "^7.6.3",
    "@babel/runtime": "^7.6.3",
    "@react-native-community/eslint-config": "^0.0.7",
    "@reason-react-native/svg": "^12.1.3",
    "babel-jest": "^24.9.0",
    "bs-platform": "^7.2.0",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.10.0",
    "flow-bin": "0.113.0",
    "jest": "^24.9.0",
    "jetifier": "^1.6.0",
    "markdown-to-json": "https://github.com/MoOx/markdown-to-json",
    "metro-react-native-babel-preset": "^0.59.0",
    "patch-package": "^6.2.0",
    "react-from-svg": "^4.0.2",
    "react-scripts": "3.2.0",
    "react-test-renderer": "^16.11.0",
    "trash-cli": "^3.0.0"
  },
  "jest": {
    "preset": "react-native"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
